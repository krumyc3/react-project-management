{
  "presets": ["es2015-node6/object-rest", "react", "stage-1"],
  "plugins": [
    "transform-decorators-legacy",
    "transform-function-bind",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": false,
    }],
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-constant-elements"
      ]
    }
  }
}